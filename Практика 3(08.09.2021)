print('Введите градусы Цельсия')
a = int(input())
print('Это', ((a*1.8)+32), 'градусов Фаренгейт')

3
print('Введите число')
a = int(input())
b = a % 7
a = a % 5
if(a > 0 and b > 0):
    print('00')
if(a > 0 and b == 0):
    print('10')
if(a == 0 and b > 0):
    print('01')
if(a == 0 and b == 0):
    print('11')
    
4
print('Введите коэффициэнт при х^2, не равный нулю')
a = float(input())
print('Введите коэффициэнт при х')
b = float(input())
print('Введите свободный член')
c = float(input())
d = (-b+((b ** 2-4*a*c)**0.5))/(2*a)
e = (-b-((b ** 2-4*a*c)**0.5))/(2*a)
if(d != e):
    print('Корни уравнения равны', d, 'и', e)
else:
    print('Корень уравнения равен',d)
