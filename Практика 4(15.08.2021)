1
print('Какая скорость транспортного средства в км/ч?')
a = int(input())
print('Сколько часов оно двигалось')
b = int(input())
print('Час       ', 'Пройденное расстояние' )
c = 1
while c != b+1:
    print(c,'        ', (c*a))
    c = c+1;
    
2
a = 1 
b = 0
print('Вводите числа')
while a > 0:
    a = int(input())
    if a > 0:
        b = b+a;
print('Их сумма равна', b)

3
a = int(input('Введите число, факториал которого надо найти '))
b = 1
c = 1
while b != a:
    b = b+1
    c = c*b;
print('Факториал числа равен', c)

4
a = int(input('Стартовое количество организмов '))
b = int(input('Среднесуточное увеличение популяции в процентах '))
c = int(input('Количество дней для размножения '))
d = 1 
print('День    Популяция')
print(1, '    ', a)
while d != c:
    d = d+1
    a = a*(1+b/100)
    print(d, '    ', a);

5
a = 0
b = int(input('Введи количество строк узора '))
while a < b:
    print('#', end=' ')
    c = 0
    while c != a:
        print(' ', end=' ')
        c = c+1
    print('#')
    a = a+1
